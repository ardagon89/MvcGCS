            }

            if ($("#Mission_statement_agreement").val() == "Complete agreement") {
                Mission_statement_agreement = 100;
                count += 1;
            }
            else if ($("#Mission_statement_agreement").val() == "Mostly agree") {
                Mission_statement_agreement = 75;
                count += 1;
            }
            else if ($("#Mission_statement_agreement").val() == "Agree as much as disagree") {
                Mission_statement_agreement = 50;
                count += 1;
            }
            else if ($("#Mission_statement_agreement").val() == "Mostly disagree") {
                Mission_statement_agreement = 25;
                count += 1;
            }
            else if ($("#Mission_statement_agreement").val() == "Do not agree") {
                Mission_statement_agreement = 0;
                count += 1;
            }

            if ($("#Vision_outcome_agreement").val() == "Complete agreement") {
                Vision_outcome_agreement = 100;
                count += 1;
            }
            else if ($("#Vision_outcome_agreement").val() == "Mostly agree") {
                Vision_outcome_agreement = 75;
                count += 1;
            }
            else if ($("#Vision_outcome_agreement").val() == "Agree as much as disagree") {
                Vision_outcome_agreement = 50;
                count += 1;
            }
            else if ($("#Vision_outcome_agreement").val() == "Mostly disagree") {
                Vision_outcome_agreement = 25;
                count += 1;
            }
            else if ($("#Vision_outcome_agreement").val() == "Do not agree") {
                Vision_outcome_agreement = 0;
                count += 1;
            }

            if ($("#Competitive_strategy_agreement").val() == "Complete agreement") {
                Competitive_strategy_agreement = 100;
                count += 1;
            }
            else if ($("#Competitive_strategy_agreement").val() == "Mostly agree") {
                Competitive_strategy_agreement = 75;
                count += 1;
            }
            else if ($("#Competitive_strategy_agreement").val() == "Agree as much as disagree") {
                Competitive_strategy_agreement = 50;
                count += 1;
            }
            else if ($("#Competitive_strategy_agreement").val() == "Mostly disagree") {
                Competitive_strategy_agreement = 25;
                count += 1;
            }
            else if ($("#Competitive_strategy_agreement").val() == "Do not agree") {
                Competitive_strategy_agreement = 0;
                count += 1;
            }

            if (count == 0) {
                $("#Company_composite_score").val(0);
            }
            else {
                $("#Company_composite_score").val((Vision_agreement + Core_values_agreement + Mission_statement_agreement + Vision_outcome_agreement + Competitive_strategy_agreement) / count);
            }
        });
    }

