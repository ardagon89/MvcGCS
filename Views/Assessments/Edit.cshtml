<script language="" type="text/javascript">
    function update_agreement_score() {
        jQuery(document).ready(function ($) {
            var Vision_agreement = 0;
            var Core_values_agreement = 0;
            var Mission_statement_agreement = 0;
            var Vision_outcome_agreement = 0;
            var Competitive_strategy_agreement = 0;
            var count = 0;

            if ($("#Vision_agreement").val() == "Complete agreement") {
                Vision_agreement = 100;
                count += 1;
            }
            else if ($("#Vision_agreement").val() == "Mostly agree") {
                Vision_agreement = 75;
                count += 1;
            }
            else if ($("#Vision_agreement").val() == "Agree as much as disagree") {
                Vision_agreement = 50;
                count += 1;
            }
            else if ($("#Vision_agreement").val() == "Mostly disagree") {
                Vision_agreement = 25;
                count += 1;
            }
            else if ($("#Vision_agreement").val() == "Do not agree") {
                Vision_agreement = 0;
                count += 1;
            }

            if ($("#Core_values_agreement").val() == "Complete agreement") {
